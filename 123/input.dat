#Declare data file type. optional {microct, raw, binary}, default is raw.
datatype: microct

#Specify data file name. (Do not give the extension file name)
datafile: 6_38
#Specify the size of image
x: 200
y: 200
z: 200

#Specify image resolution (µm). The default is 3.
precision: 6.55
#Specify minimum R*R of pores. The default is 2.
minr2: 1
